{% extends 'GrabagameBookingBundle:Admin:dashboard.html.twig' %}

{% block title %}Club added{% endblock %}

{% block admin_content %}
    <h1>Successfully added a club</h1>

    <h3>Details</h3>
    <strong>Name:</strong> {{ Club.getName }}
    <strong>Email:</strong> {{ Club.getEmail }}
    <strong>Booking increment:</strong> {{ Club.getBookingIncrement }}
    <strong>Number of courts:</strong> {{ Club.getNumberOfCourts }}

{% endblock %}

{% block sidebar %}{% endblock %}
